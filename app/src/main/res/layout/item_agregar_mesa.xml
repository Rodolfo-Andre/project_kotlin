<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cv"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:animateLayoutChanges="true"
    app:cardCornerRadius="20dp"
    app:cardElevation="4dp"
    app:cardUseCompatPadding="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:animateLayoutChanges="true"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:animateLayoutChanges="true"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView60"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="291dp"
                android:layout_marginTop="14dp"
                android:layout_marginEnd="46dp"
                android:layout_marginBottom="167dp"
                android:text="Estado:"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtrucEstablecimientoList"
                android:layout_width="90dp"
                android:layout_height="19dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="291dp"
                android:layout_marginTop="37dp"
                android:layout_marginEnd="11dp"
                android:layout_marginBottom="144dp"
                android:text="TextView" />

            <TextView
                android:id="@+id/textView35"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="92dp"
                android:layout_marginTop="14dp"
                android:layout_marginEnd="244dp"
                android:layout_marginBottom="167dp"
                android:text="Codigo:"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtcodListadoEstablecimineto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="92dp"
                android:layout_marginTop="35dp"
                android:layout_marginEnd="244dp"
                android:layout_marginBottom="146dp"
                android:text="TextView" />

            <TextView
                android:id="@+id/textView56"
                android:layout_width="64dp"
                android:layout_height="19dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="189dp"
                android:layout_marginTop="14dp"
                android:layout_marginEnd="139dp"
                android:layout_marginBottom="167dp"
                android:text="Asientos:"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtnombreEstablecimientoList"
                android:layout_width="wrap_content"
                android:layout_height="21dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="189dp"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="131dp"
                android:layout_marginBottom="143dp"
                android:text="TextView" />

            <View
                android:id="@+id/divider15"
                android:layout_width="3dp"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="19dp"
                android:layout_marginEnd="120dp"
                android:layout_marginBottom="140dp"
                android:background="?android:attr/listDivider" />

            <View
                android:id="@+id/divider16"
                android:layout_width="3dp"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="219dp"
                android:layout_marginBottom="140dp"
                android:background="?android:attr/listDivider" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="37dp"
                android:layout_height="35dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="21dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="334dp"
                android:layout_marginBottom="145dp"
                app:srcCompat="@drawable/insertar_mesa_50" />

        </RelativeLayout>

    </LinearLayout>
</androidx.cardview.widget.CardView>